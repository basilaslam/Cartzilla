<script>

  const socket = io.connect('/marketplace/show-nft')

  // Updating date
  let date = "<%= nft.created_date %>"
  let limit = "<%= nft.duration %>"

  document.getElementById('auction_end').innerText = moment(date).add(limit).format("dddd, MMMM Do YYYY")


  socket.on('connect', () => {
    console.log(socket.id)
  })

  socket.emit('joinauctionroom', "<%= nft.id%>");

  socket.on('bid', (msg) => {
    console.log(msg, 'bid');
  });
  socket.on('bidded', function (msg) {
    let currentBid = document.getElementById('currentBid')
    currentBid.innerText = `${msg.bid} ETH`

    console.log(msg, '----> ', msg.id)
  });


  document.getElementById('bidButton').addEventListener('click', async (el) => {
    let amount = document.getElementById('bidAmount').value
    let currentBid = document.getElementById('currentBid').innerText.split(' ')[0]

    console.log(Number(amount) > Number(currentBid))
    console.log(amount, currentBid)

    if (Number(amount) > Number(currentBid)) {
      //Updating Current Bid
      document.getElementById('currentBid').innerText = `${amount} ETH`
      socket.emit('bid', {
        id: "<%= nft.id%>",
        bid: amount,
        user: "<%= userData._id%>"
      })
    }


  })








</script>
