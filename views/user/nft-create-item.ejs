      style="display: none; visibility: hidden"></iframe>
  </noscript>

  <body class="handheld-toolbar-enabled">
    <%- include('common/navbar.ejs') %>
      <% console.log(userData) %>
        <main class="page-wrapper" style="margin-top: 80px">
          <!-- Navbar for NFT Marketplace demo-->
          <!-- Page title-->
          <div class="bg-accent mb-4 py-4">
            <div class="container d-lg-flex justify-content-between py-2 py-lg-3">
              <div class="order-lg-2 mb-3 mb-lg-0 pt-lg-2">
                <nav aria-label="breadcrumb">
                  <ol
                    class="breadcrumb breadcrumb-light flex-lg-nowrap justify-content-center justify-content-lg-start">
                    <li class="breadcrumb-item">
                      <a class="text-nowrap" href="index.html"><i class="ci-home"></i>Home</a>
                    </li>
                    <li class="breadcrumb-item text-nowrap">
                      <a href="home.ejs">NFT Marketplace</a>
                    </li>
                    <li class="breadcrumb-item text-nowrap active" aria-current="page">
                      Create New Item
                    </li>
                  </ol>
                </nav>
              </div>
              <div class="order-lg-1 pe-lg-4 text-center text-lg-start">
                <h1 class="h3 text-light mb-0">Create New Item</h1>
              </div>
            </div>
          </div>
          <!-- Page content-->
          <section class="container mb-5 pt-3 pb-md-5 pb-1">
            <div class="row gy-5">
              <form action="/user/create-nft" method="post" enctype="multipart/form-data"
                class="needs-validation col-md-8">
                <!-- Upload file-->
                <div class="mb-md-5 mb-4 pb-md-0 pb-2">
                  <h2 class="h5 mb-2 pb-1">Upload file</h2>
                  <p class="mb-4 fs-sm">
                    File types supported: PNG, JPG, GIF, WEBP, MP3 or MP4. Max size:
                    100mb.
                  </p>
                  <!-- Drag and drop file upload-->
                  <div class="file-drop-area">
                    <div class="file-drop-icon ci-cloud-upload"></div>
                    <span class="file-drop-message">Drag and drop here to upload</span>
                    <input name="filetoupload" class="file-drop-input" type="file" />
                    <button class="file-drop-btn btn btn-outline-accent" type="button">
                      Or select file
                    </button>
                  </div>
                </div>
                <!-- Details-->
                <div class="mb-md-5 mb-4 pb-md-0 pb-2">
                  <h2 class="h5 mb-3">Details</h2>
                  <div class="row gy-md-4 gy-3">
                    <div class="col-12">
                      <label class="form-label" for="details-title">Title<span class="ms-1 text-danger">*</span></label>
                      <input name="name" class="form-control" id="details-title" type="Text" placeholder="Item title"
                        required />
                      <div class="invalid-feedback">Please, enter the title.</div>
                      <span class="form-text">Maximum 100 characters. No HTML or emoji allowed.</span>
                    </div>
                    <div class="col-sm-6">
                      <label class="form-label" for="details-category">Category</label>
                      <select name="category" class="form-select" id="details-category">
                        <option disabled selected>Choose category</option>
                        <option value="Art">Art</option>
                        <option value="Photography">Photography</option>
                        <option value="Music">Music</option>
                        <option value="Gaming">Gaming</option>
                        <option value="Sports">Sports</option>
                        <option value="Collections">Collections</option>
                        <option value="Utility">Utility</option>
                      </select>
                    </div>
                    <div class="col-sm-6">
                      <label class="form-label" for="details-collection">Add to collection</label>
                      <select name="collection" class="form-select" id="details-collection">
                        <option disabled selected>Choose collection</option>
                        <option value="Contemporary art collage">
                          Contemporary art collage
                        </option>
                        <option value="3D digital abstract art">
                          3D digital abstract art
                        </option>
                        <option value="Clone X Mini Monsters">
                          Clone X Mini Monsters
                        </option>
                        <option value="Ocean and sky">Ocean and sky</option>
                        <option value="Aesthetic art collage">
                          Aesthetic art collage
                        </option>
                      </select>
                    </div>
                    <div class="col-12">
                      <label class="form-label" for="details-description">Description</label>
                      <textarea class="form-control" id="details-description" rows="4"
                        placeholder="Enter a short description of your item"></textarea><span class="form-text">0 of 500
                        characters used.</span>
                    </div>
                  </div>
                </div>
                <!-- Price & payment options-->
                <div>
                  <h2 class="h5 mb-4 pb-md-2">Price &amp; payment options</h2>
                  <!-- Button tabs-->
                  <div class="mb-4" style="overflow-x: auto">
                    <div class="nav nav-tabs d-inline-flex ms-n1 mb-0 border-0 flex-nowrap text-nowrap" role="tablist">
                      <div class="btn btn-outline-accent active position-relative ms-1 me-2 mb-2"
                        data-bs-target="#price-fix" data-bs-toggle="tab" role="tab">
                        <i class="ci-lable me-2"></i>Fixed price
                        <label class="position-absolute top-0 start-0 w-100 h-100" style="cursor: pointer">
                          <input class="visually-hidden" type="radio" name="price" checked />
                        </label>
                      </div>
                      <div class="btn btn-outline-accent position-relative ms-1 me-2 mb-2" data-bs-target="#price-open"
                        data-bs-toggle="tab" role="tab">
                        <i class="ci-auction me-2"></i>Open for bids
                        <label class="position-absolute top-0 start-0 w-100 h-100" style="cursor: pointer">
                          <input class="visually-hidden" type="radio" name="price" />
                        </label>
                      </div>
                      <div class="btn btn-outline-accent position-relative ms-1 mb-2" data-bs-target="#price-time"
                        data-bs-toggle="tab" role="tab">
                        <i class="ci-time me-2"></i>Time auctions
                        <label class="position-absolute top-0 start-0 w-100 h-100" style="cursor: pointer">
                          <input class="visually-hidden" type="radio" name="price" />
                        </label>
                      </div>
                    </div>
                  </div>
                  <!-- Tabs content-->
                  <div class="tab-content">
                    <!-- Fixed price tab-->
                    <div class="tab-pane fade mb-4 pb-2 show active" id="price-fix" role="tabpanel">
                      <div class="row gy-md-4 gy-3 mb-md-5 mb-4 pb-md-0 pb-2">
                        <div class="col-sm-6">
                          <label class="form-label">Price</label>
                          <div class="input-group">
                            <input name="price" class="form-control" type="text" placeholder="Enter price" />
                            <div class="dropdown" data-bs-toggle="select">
                              <select class="form-select" name="price_fix_currency">
                                <option value="ETH">ETH</option>
                                <option value="BNB">BNB</option>
                                <option value="BUSD">BUSD</option>
                              </select>

                            </div>
                          </div>
                        </div>
                        <div class="col-sm-6">
                          <label class="form-label">Preferred payment type</label>
                          <select class="form-select" name="payment_type">
                            <option value="ETH">ETH</option>
                            <option value="BNB">BNB</option>
                            <option value="BUSD">BUSD</option>
                          </select>
                        </div>
                        <div class="col-sm-6">
                          <label class="form-label" for="price-fix-payout">Payout to address</label>
                          <input name="wallet_address" class="form-control" id="price-fix-payout" type="text"
                            placeholder="eg. 0x1dDB2C08s97...9Ec" />
                        </div>
                        <div class="col-sm-6">
                          <div class="form-check mt-sm-4 pt-sm-3">
                            <input class="form-check-input" id="price-fix-wallet" type="checkbox" checked />
                            <label class="form-check-label fs-base" for="price-fix-wallet"><span class="me-2">Use
                                address currently connected to wallet</span><i
                                class="ci-wallet fs-lg mt-n1 align-middle text-muted"></i></label>
                          </div>
                        </div>
                      </div>
                    </div>
                    <!-- Open for bids tab-->
                    <div class="tab-pane fade mb-4 pb-2" id="price-open" role="tabpanel">
                      <div class="row gy-md-4 gy-3 mb-md-5 mb-4 pb-md-0 pb-2">
                        <div class="col-12">
                          <label class="form-label" for="price-open-min">Minimum bid</label>
                          <div class="input-group">
                            <input class="form-control" id="price-open-min" type="text"
                              placeholder="Enter minimum bid" />
                            <div class="dropdown" data-bs-toggle="select">
                              <button class="btn btn-outline-secondary dropdown-toggle rounded-0 rounded-end"
                                type="button" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span class="dropdown-toggle-label">ETH</span>
                              </button>
                              <div class="dropdown-menu dropdown-menu-end w-100" style="min-width: auto">
                                <a class="dropdown-item" href="#"><span class="dropdown-item-label">ETH</span></a><a
                                  class="dropdown-item" href="#"><span class="dropdown-item-label">BNB</span></a><a
                                  class="dropdown-item" href="#"><span class="dropdown-item-label">BUSD</span></a>
                              </div>
                            </div>
                          </div>
                        </div>
                        <div class="col-sm-6">
                          <label class="form-label" for="price-open-start">Starting date</label>
                          <div class="input-group">
                            <input class="form-control date-picker date-range rounded pe-5" id="price-open-start"
                              type="text" placeholder="Choose date and time"
                              data-datepicker-options='{"enableTime": true, "altInput": true, "altFormat": "F j, Y", "dateFormat": "Y-m-d"}'
                              data-linked-input="#price-open-end" /><i
                              class="ci-calendar position-absolute top-50 end-0 translate-middle-y me-3"></i>
                          </div>
                        </div>
                        <div class="col-sm-6">
                          <label class="form-label" for="price-open-end">Expiration date</label>
                          <div class="input-group">
                            <input class="form-control date-picker rounded pe-5" id="price-open-end" type="text"
                              placeholder="Choose date and time"
                              data-datepicker-options='{"enableTime": true, "altInput": true, "altFormat": "F j, Y", "dateFormat": "Y-m-d"}' /><i
                              class="ci-calendar position-absolute top-50 end-0 translate-middle-y me-3"></i>
                          </div>
                        </div>
                        <div class="col-sm-6">
                          <label class="form-label" for="price-open-payout">Payout to address</label>
                          <input class="form-control" id="price-open-payout" type="text"
                            placeholder="eg. 0x1dDB2C08s97...9Ec" />
                        </div>
                        <div class="col-sm-6">
                          <div class="form-check mt-sm-4 pt-sm-3">
                            <input class="form-check-input" id="price-open-wallet" type="checkbox" checked />
                            <label class="form-check-label fs-base" for="price-open-wallet"><span class="me-2">Use
                                address currently connected to wallet</span><i
                                class="ci-wallet fs-lg mt-n1 align-middle text-muted"></i></label>
                          </div>
                        </div>
                      </div>
                      <!-- Properties-->

                      <div class="row gy-md-4 gy-3 mb-3">
                        <div class="col-sm-6">
                          <input class="form-control" type="text" placeholder="eg. Size" />
                        </div>
                        <div class="col-sm-6">
                          <input class="form-control" type="text" placeholder="eg. M" />
                        </div>
                      </div>
                      <a href="#"><i class="ci-add me-2"></i><ins>Add more</ins></a>
                    </div>
                    <!-- Time auctions tab-->
                    <div class="tab-pane fade" id="price-time" role="tabpanel">
                      <p>Time auctions settings go here</p>
                    </div>
                  </div>
                  <!-- Submit-->
                  <div class="pt-2">
                    <button class="btn btn-lg btn-accent d-block w-100" type="submit">
                      Create item
                    </button>
                  </div>
                </div>
              </form>
              <!-- Preview-->
              <aside class="col-lg-3 offset-lg-1 col-md-4 position-relative" style="z-index: 1">
